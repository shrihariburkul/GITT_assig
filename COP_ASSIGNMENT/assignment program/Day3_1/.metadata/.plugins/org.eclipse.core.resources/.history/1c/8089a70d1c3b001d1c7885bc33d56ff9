package arrayProgram;

public class addTwoarry {
  public static void main(String []args) {
	  int a[]= {1,2,3,4,5};
	  int b[]= {5,6,7,8,9};
	  
	if(a.length==b.length) {
		int c[]=new int[a.length];
	for( int i=0;i<a.length;i++)
	
	{
		c[i]=a[i]+b[i];
		
	}
		int i = 0;
		System.out.println(" "+c[i]);
		
      }
    }
}