package Day4_1;

public class abcdpattern {
	
 public static void main(String[] args) {  
		char r='A';
	    
		for( int i=1;i<=5;i++)
	{
		 r='A';
	
		for( int j=1;j<=i;j++)	
		{
		System.out.print(r);
		     r++;
		 }
		System.out.println();
	 }
	}
  }
	
	  /*  public static void main (String[] args)
	    {
	        int i, j, n = 5;
	        for (i = 1; i <= n; i++)
	        {
	            int ch = 65;
	            for (j = 1; j <= i; j++)
	            {
	                System.out.print(((char)ch));
	                ch++;
	            }
	        System.out.println();
	        }
	    }
	}
