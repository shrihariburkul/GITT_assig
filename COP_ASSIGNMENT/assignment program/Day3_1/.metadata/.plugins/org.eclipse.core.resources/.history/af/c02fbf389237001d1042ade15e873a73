package Day3_1;

import java.util.Scanner;

public class Empolyee {

	public static void main(String[] args) {

		String Name = null;
		Double Salary = null;
		String Address = null;
		int Joinyear = 0;

		Scanner sc = new Scanner(System.in);
		for (int i = 0; i < 3; i++) {
			System.out.println("Enter Name, Salary, Address, Joinyear");

			Name = sc.next();
			Salary = sc.nextDouble();
			Address = sc.next();
			Joinyear = sc.nextInt();

			System.out.println(
					"Name : " + Name + " Salary  : " + Salary + " Address : " + Address + " Joinyear : " + Joinyear);

		}
	}
}
