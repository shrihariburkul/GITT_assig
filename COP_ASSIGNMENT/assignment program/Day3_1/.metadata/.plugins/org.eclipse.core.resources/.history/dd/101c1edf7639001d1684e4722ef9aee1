package Day4_1;

import java.util.Scanner;

public class pattern {

	public static void main(String[] args) {
		int n[]=new int[10];
		int sum=0;
		int num;
		Scanner sc= new Scanner(System.in);
		for(int i=0;i<=10;i++)
		{
			System.out.println("enter the num");
			num =sc.nextInt();
		}
		for(int i=0;i<=10;i++)
		{
			System.out.println(" "+n[i]);
					
	}
	}
}
