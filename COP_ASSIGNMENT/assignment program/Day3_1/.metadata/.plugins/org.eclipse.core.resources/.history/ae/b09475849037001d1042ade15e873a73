package Day3_1;

import java.util.Scanner;

public class Empolyee {
	String Name;
	Double Salary;
	String Address;
	int Joinyear;

	public static void main(String[] args) {

		Scanner sc = new Scanner(System.in);
		for (int i = 1; i < 4; i++) {
			System.out.println("Enter Name, Salary, Address, Joinyear");

			Name = sc.next();
			Salary = sc.nextDouble();
			Address = sc.next();
			Joinyear = sc.nextInt();
		

		System.out.println(
				"Name : " + Name + " Salary  : " + Salary + " Address : " + Address + "Joinyear : " + Joinyear);
		}
	}

}
