package Day3_1;

import java.util.Scanner;

public class Empolyee {

	public static void main(String[] args) {

		String Name;
		Double Salary;
		String Address;
		int Joinyear;

		Scanner sc = new Scanner(System.in);
int i = 3;
do {
		System.out.println("Enter Name, Salary, Address, Joinyear");

		Name = sc.next();
		Salary = sc.nextDouble();
		Address = sc.next();
		Joinyear = sc.nextInt();
}while(i<=4);

		for (int j = 1; j < 4; j++)

			System.out.println("Name : " + Name + " Salary  : " + Salary + " Address : " + Address + "Joinyear : " + Joinyear);

	}

}
