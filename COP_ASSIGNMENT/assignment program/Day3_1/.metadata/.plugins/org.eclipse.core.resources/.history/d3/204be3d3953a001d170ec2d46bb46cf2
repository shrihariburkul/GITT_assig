package Day3_1;

import java.util.Scanner;

public class VowelConsonantSwitchcase {

	public static void main(String[] args) {
		
		
		
		char ch ='k';
		 
		switch(ch) {
	
		case  'a':
		case  'e':
		case  'i':
		case  'O':
		case  'u':
			
			System.out.println(ch+ " is vowel");
			break ;
			
		default :	
			System.out.println(ch+ " is consonant");
			
			
		} 		
	}
}
