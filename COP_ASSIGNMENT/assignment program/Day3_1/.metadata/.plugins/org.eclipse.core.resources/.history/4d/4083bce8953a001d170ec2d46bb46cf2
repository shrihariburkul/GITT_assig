package Day4_1;

import java.util.Scanner;

public class tableOfthenum {

	public static void main(String[] args) {

		int num;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the num ");

		num = sc.nextInt();
		for (int i = 1; i <= num; i++) {
			System.out.println(num + "*" + i + "=" + (num * i ));
		}
	}
}
