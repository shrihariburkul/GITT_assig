package Day4_1;

import java.util.Scanner;

public class factorial {

	public static void main(String[] args) {
		int num;
		int fact = 1;
		Scanner sc = new Scanner(System.in);
		System.out.println("Enter the num ");
     
		num= sc.nextInt();
		for (int i = 1; i <= num; i++) {
			fact = fact * i;
		}
		System.out.println(fact);
	}

}
