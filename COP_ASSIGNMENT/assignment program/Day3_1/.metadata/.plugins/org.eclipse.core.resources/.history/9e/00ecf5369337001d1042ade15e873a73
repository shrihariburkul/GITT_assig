package Day3_1;

import java.util.Scanner;

public class Employee {

	public static void main(String[] args) {

		String Name;
		Double Salary;
		String Address;
		int Joinyear;

		// \n
		
		Scanner sc = new Scanner(System.in);
		for (int i = 0; i < 3; i++) {
			System.out.println("Enter Name, Salary, Address, Joinyear : ");

			Name = sc.next();
			Salary = sc.nextDouble();
			Address = sc.next();
			Joinyear = sc.nextInt();

			System.out.println(
					"Name : " + "  " + " Salary  : " + Salary + " Address : " + Address + " Joinyear : " + Joinyear);
			System.out.println();
		}
	}
}


