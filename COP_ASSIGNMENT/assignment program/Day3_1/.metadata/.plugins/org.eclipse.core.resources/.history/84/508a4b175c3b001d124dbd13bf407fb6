package assig4;

public class palindrome {

	public static void main(String[] args) {
		
		
		int j = 0;
		String s= new String("MADAM");
		int l= s.length()/2;
	for(int i=0;i<=l;i++,j--)
	{
		if(s.charAt(i)!=s.charAt(j))
		{
			System.out.println("String is not palindrom");
			break;
		}
	
if (i==l)
	System.out.println("String is palindrom");
	}
	}
}
